{ "include": [
  "src/**/*.ts"
],
"exclude": [
  "node_modules",
  "lib"
],
  "compilerOptions": {
    "target": "es5",
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node",
    "jsx": "react",
    "declaration": true,
    "sourceMap": true,
    "experimentalDecorators": true,
    "skipLibCheck": true,
    "outDir": "lib",
    "module": "esnext",
    "typeRoots": [
      "./node_modules/@types",
      "./node_modules/@microsoft"
    ],
    "types": [
      "es6-promise",
      "webpack-env",
      "csv-parse",
      "microsoft-ajax",
      "sharepoint"

    ],
    "lib": [
      "es5",
      "dom",
      "es2015.collection"
    ]
   
  }
}
