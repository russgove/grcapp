        }
      ]
    },
    "user-access-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/userAccess/UserAccessWebPart.js",
          "manifest": "./src/webparts/userAccess/UserAccessWebPart.manifest.json"
        }
      ]
    },
    "user-access-site-setup-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/userAccessSiteSetup/UserAccessSiteSetupWebPart.js",
          "manifest": "./src/webparts/userAccessSiteSetup/UserAccessSiteSetupWebPart.manifest.json"
        }
      ]
    }
  },
  "externals": {
    "sp-init": {
      "path": "https://tronoxglobal.sharepoint.com/_layouts/15/init.js",
      "globalName": "$_global_init"
    },
    "microsoft-ajax": {
      "path": "https://tronoxglobal.sharepoint.com/_layouts/15/MicrosoftAjax.js",
      "globalName": "Sys",
      "globalDependencies": [
        "sp-init"
      ]
    },
    "sp-runtime": {
      "path": "https://tronoxglobal.sharepoint.com/_layouts/15/SP.Runtime.js",
      "globalName": "SP",
      "globalDependencies": [
        "microsoft-ajax"
      ]
    },
    "sharepoint": {
      "path": "https://tronoxglobal.sharepoint.com/_layouts/15/SP.js",
      "globalName": "SP",
      "globalDependencies": [
        "sp-runtime"
      ]
    },
    "sp-workflow": {
      "path": "https://tronoxglobal.sharepoint.com/_layouts/15/SP.WorkflowServices.js",
      "globalName": "SP",
      "globalDependencies": [
        "sp-runtime"
      ]
    }
  },
  "localizedResources": {
    "GrcTestWebPartStrings": "lib/webparts/grcTest/loc/{locale}.js",
    "GrcUploadWebPartStrings": "lib/webparts/grcUpload/loc/{locale}.js",
    "HighRiskAdminWebpartWebPartStrings": "lib/webparts/highRiskAdminWebpart/loc/{locale}.js",
    "HighRiskUserWebpartWebPartStrings": "lib/webparts/highRiskUserWebpart/loc/{locale}.js",
    "GrcAzureManagementWebPartStrings": "lib/webparts/grcAzureManagement/loc/{locale}.js",
    "MitigatingControlsSiteSetupWebPartStrings": "lib/webparts/mitigatingControlsSiteSetup/loc/{locale}.js",
    "MitigatingControlsWebPartStrings": "lib/webparts/mitigatingControls/loc/{locale}.js",
    "PropertyControlStrings": "./node_modules/@pnp/spfx-property-controls/lib/loc/{locale}.js",
    "BusinessRoleReviewWebPartStrings": "lib/webparts/businessRoleReview/loc/{locale}.js",
    "BusinessRoleReviewSiteSetupWebPartStrings": "lib/webparts/businessRoleReviewSiteSetup/loc/{locale}.js",
    "HighRiskWebPartStrings": "lib/webparts/highRisk/loc/{locale}.js",
    "HighRiskSiteSetupWebPartStrings": "lib/webparts/highRiskSiteSetup/loc/{locale}.js",
    "UserAccessWebPartStrings": "lib/webparts/userAccess/loc/{locale}.js",
    "UserAccessSiteSetupWebPartStrings": "lib/webparts/userAccessSiteSetup/loc/{locale}.js"
  }
}
