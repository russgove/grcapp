{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "mitigating-controls-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/mitigatingControls/MitigatingControlsWebPart.js",
          "manifest": "./src/webparts/mitigatingControls/MitigatingControlsWebPart.manifest.json"
        }
      ]
    },
    "business-role-review-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/businessRoleReview/BusinessRoleReviewWebPart.js",
          "manifest": "./src/webparts/businessRoleReview/BusinessRoleReviewWebPart.manifest.json"
        }
      ]
    },
    "user-access-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/userAccess/UserAccessWebPart.js",
          "manifest": "./src/webparts/userAccess/UserAccessWebPart.manifest.json"
        }
      ]
    },
    "role-to-t-code-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/roleToTCode/RoleToTCodeWebPart.js",
          "manifest": "./src/webparts/roleToTCode/RoleToTCodeWebPart.manifest.json"
        }
      ]
    }
  },
  "externals": {
    "sp-init": {
      "path": "https://tronoxglobal.sharepoint.com/_layouts/15/init.js",
      "globalName": "$_global_init"
    },
    "microsoft-ajax": {
      "path": "https://tronoxglobal.sharepoint.com/_layouts/15/MicrosoftAjax.js",
      "globalName": "Sys",
      "globalDependencies": [
        "sp-init"
      ]
    },
    "sp-runtime": {
      "path": "https://tronoxglobal.sharepoint.com/_layouts/15/SP.Runtime.js",
      "globalName": "SP",
      "globalDependencies": [
        "microsoft-ajax"
      ]
    },
    "sharepoint": {
      "path": "https://tronoxglobal.sharepoint.com/_layouts/15/SP.js",
      "globalName": "SP",
      "globalDependencies": [
        "sp-runtime"
      ]
    },
    "sp-workflow": {
      "path": "https://tronoxglobal.sharepoint.com/_layouts/15/SP.WorkflowServices.js",
      "globalName": "SP",
      "globalDependencies": [
        "sp-runtime"
      ]
    }
  },
  "localizedResources": {

    "MitigatingControlsWebPartStrings": "lib/webparts/mitigatingControls/loc/{locale}.js",
    "PropertyControlStrings": "./node_modules/@pnp/spfx-property-controls/lib/loc/{locale}.js",
    "BusinessRoleReviewWebPartStrings": "lib/webparts/businessRoleReview/loc/{locale}.js",
    "UserAccessWebPartStrings": "lib/webparts/userAccess/loc/{locale}.js",
    "RoleToTCodeWebPartStrings": "lib/webparts/roleToTCode/loc/{locale}.js",

  }
}
