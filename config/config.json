        }
      ]
    },
    "high-risk-admin-webpart-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/highRiskAdminWebpart/HighRiskAdminWebpartWebPart.js",
          "manifest": "./src/webparts/highRiskAdminWebpart/HighRiskAdminWebpartWebPart.manifest.json"
        }
      ]
    },
    "high-risk-user-webpart-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/highRiskUserWebpart/HighRiskUserWebpartWebPart.js",
          "manifest": "./src/webparts/highRiskUserWebpart/HighRiskUserWebpartWebPart.manifest.json"
        }
      ]
    },
    "grc-azure-management-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/grcAzureManagement/GrcAzureManagementWebPart.js",
